<!DOCTYPE html>
<html>
  <head>
    <title>Users for <%= shopData.shopName %></title>
    <link rel="stylesheet" type="text/css" href="/main.css">
  </head>
  <body>
    <h1>Users Registered at <%= shopData.shopName %></h1>

    <table border="1" cellpadding="8">
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>

      <% for (var i = 0; i < users.length; i++) { 
           var user = users[i]; %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.first %></td>
          <td><%= user.last %></td>
          <td><%= user.email %></td>
        </tr>
      <% } %>
    </table>

  </body>
</html>

